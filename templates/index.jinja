{% extends "base.jinja" %}
{% block content %}
    {{ page.content }}
    <p>
    <div class="social-links">
        <a href="https://github.com/edsaac" target="_blank" title="GitHub page">
            <img src="https://img.shields.io/badge/GitHub-181717?style=flat&logo=github&logoColor=white"
                alt="GitHub">
        </a>
        <a href="https://www.linkedin.com/in/edsaac" target="_blank" title="LinkedIn profile">
            <img src="https://img.shields.io/badge/LinkedIn-0077B5?style=flat&logo=data:image/svg%2bxml;base64,&logoColor=white"
                alt="LinkedIn">
        </a>
        <a href="https://orcid.org/0000-0003-1242-4815" target="_blank" title="ORCiD profile">
            <img src="https://img.shields.io/badge/ORCiD-A6CE39?style=flat&logo=orcid&logoColor=white"
                alt="ORCiD profile">
        </a>
    </div>
    </p>
    
<div class='divider'></div>

{% for photo in photos %}
    <h3 id="{{ photo.id }}">
        > {{ photo.title }}
    </h3>
    <div class="photo-info">
        <div class="photo-info-img">
            <a href="{{ photo.location }}" target="_blank">
                <img src="{{ photo.url }}" alt="alternatetext" style="width: 100%;">
            </a>
        </div>
        <div class="photo-info-text">
            <p>
                [{{ photo.date }}] {{ photo.caption }}
            </p>
        </div>
    </div>
    {% if not loop.last %}
        <div class='divider'></div>
    {% endif %}
{% endfor %}
{% endblock %}